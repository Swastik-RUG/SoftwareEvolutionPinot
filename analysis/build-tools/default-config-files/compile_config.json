[
 {
  "compiler": "clang",
  "compile-commands-file-name": "compile_commands.json",
  "excludes-file-name": "excludes.txt",
  "ast-root-folder-name": "generated-asts",
  "output-file-name": "structure101-model.cpa",
  "compilerArguments": [
   "-E",
   "-g",
   "-Xclang",
   "-ast-dump",
   "-w",
   "-fno-color-diagnostics",
   "-Wno-invalid-constexpr"
  ],
  "argumentsToDelete": [
   "(.*)g[++](.*)",
   "^gcc$",
   "(.*)cc.exe",
   "(.*)/cc",
   "(.*)\\\\cc",
   "^cc$",
   "(.*)CC.exe",
   "(.*)/CC",
   "(.*)\\\\CC",
   "^CC",
   "(.*)c89.exe",
   "(.*)/c89",
   "(.*)\\\\c89",
   "^c89$",
   "(.*)c99.exe",
   "(.*)/c99",
   "(.*)\\\\c99",
   "^c99$",
   "(.*)c11.exe",
   "(.*)/c11",
   "(.*)\\\\c11",
   "^c11$",
   "(.*)c14.exe",
   "(.*)/c14",
   "(.*)\\\\c14",
   "^c14$",
   "(.*)c18.exe",
   "(.*)/c18",
   "(.*)\\\\c18",
   "^c18$",
   "(.*)ccpentium.exe",
   "(.*)/ccpentium",
   "(.*)\\\\ccpentium",
   "^ccpentium$",
   "(.*)cl.exe",
   "(.*)clang-tool.exe",
   "(.*)/clang-tool",
   "(.*)\\\\clang-tool",
   "^clang-tool$",
   "-std=gnu++11",
   "-std=gnu++14",
   "-c",
   "/c",
   "-fcolor-diagnostics",
   "-fstack-reuse=none",
   "-fno-extended-identifiers",
   "-fno-tree-scev-cprop",
   "/EH(.*)",
   "/Fd(.*)",
   "/FS",
   "/GF",
   "/GS",
   "/GS-",
   "/GT",
   "/Gy",
   "/Gy-",
   "/MD",
   "/MP",
   "/MT",
   "/O1",
   "/O2",
   "/Ob0",
   "/Ob1",
   "/Ob2",
   "/Ob3",
   "/Oi",
   "/Os",
   "/Ot",
   "/TP",
   "/TC",
   "-W(.*)",
   "/W(.*)",
   "/w(.*)",
   "/Z(.*)",
   "/bigobj",
   "/nologo",
   "/utf-8"
  ],
  "argumentsToReplace": [
    {
      "pattern": "/arch:SSE2",
      "replace": "-msse2"
    },
    {
      "pattern": "/GR-",
      "replace": "-fnortti"
    },
    {
      "pattern": "/GR",
      "replace": "-frtti"
    },
    {
      "pattern": "/D(.*)",
      "replace": "-D$1"
    },
   {
    "pattern": "/I(.*)",
    "replace": "-I$1"
   }
   ]
 }
]
